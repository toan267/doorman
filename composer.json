{
    "name": "toan267/doorman",
    "description": "Child process management",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "jeremeamia/SuperClosure": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "AsyncPHP\\Doorman\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsyncPHP\\Doorman\\Tests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "toan267/doorman",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/toan267/doorman.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-4": {
                        "AsyncPHP\\Doorman\\": "src"
                    }
                }
            }
        }
    ]
}
